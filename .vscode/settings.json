{
    "mypy.configFile": "pyproject.toml",
    "flake8.args": [
        "--config=../.flake8"
    ],
    "mypy.dmypyExecutable": "/usr/bin/dmypy",
    // "mypy.dmypyExecutable": "/run/media/serhappy/Data/Projects/Python/CarWebBot/.venv/bin/dmypy",
    "isort.args": [
        "--profile",
        "black"
    ],
    "cSpell.words": [
        "подпапки",
        "Addedfile",
        "Dcisd",
        "dropzone",
        "eventlet",
        "LOGURU",
        "Pydantic",
        "pytz",
        "queuecomplete",
        "takeoff",
        "uvicorn"
    ],
    "[html][django-html][handlebars][hbs][mustache][jinja][jinja-html][nj][njk][nunjucks][twig]": {
        "editor.defaultFormatter": "monosans.djlint",
    },
    "[django-html]": {
        "editor.defaultFormatter": "monosans.djlint"
    },
    "git.decorations.enabled": true,
    "python.linting.flake8Enabled": false,
    "python.analysis.typeCheckingMode": "off",
}
